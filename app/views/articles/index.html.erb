<h1>Articles</h1>
<ul>
  <% @articles.each do |article| %>
    <li><%= link_to "#{article.title}", article_path(article) %> </li>
    <li><%= link_to "Delete #{article.title}",
            article_path(article),
            method: :delete,
            data: { confirm: "Are you sure?" } %></li>
  <% end %>
</ul>

<%= link_to 'Create a new article', new_article_path %>


